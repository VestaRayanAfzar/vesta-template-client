{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "importHelpers": false,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["esnext", "dom"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "resolveJsonModule": true,
    "skipLibCheck": false,
    "target": "es6"
  },
  "include": ["src"]
}
